#!/usr/bin/env ruby

STDOUT.sync = true
STDERR.sync = true

require File.expand_path('../../config/environment',  __FILE__)
require 'adhearsion/cli_commands'

Thread.abort_on_exception = true # Set to true for debugging

Dir['./lib/*.rb'].each { |f| require f }

AmqpManager.start
Adhearsion::CLI::AhnCommand.start
